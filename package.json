{
	"name": "@uteam-tw/gogovan-plus",
	"version": "1.0.0",
	"description": "gogovan plus api wrapper",
	"main": "./lib/src/index.js",
	"scripts": {
		"prepublish": "npm run build",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build": "npm run build:types && npm run build:js",
		"prebuild": "rm -r ./lib || true",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "babel src --out-dir lib/src --extensions \".ts,.tsx\" --source-maps inline",
		"prestart": "npm run build:types",
		"start": "babel src --watch --out-dir lib/src --extensions \".ts,.tsx\" --source-maps inline",
		"test": "jest",
		"pretest": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/uteam-tw/node-gogovan-plus.git"
	},
	"keywords": [
		"gogovan",
		"node",
		"gogovan-plus",
		"api"
	],
	"author": "uteam-tw (https://github.com/uteam-tw)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/uteam-tw/node-gogovan-plus/issues"
	},
	"homepage": "https://github.com/uteam-tw/node-gogovan-plus#readme",
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/jest": "^26.0.7",
		"babel-jest": "^26.1.0",
		"date-fns": "^2.15.0",
		"dotenv": "^8.2.0",
		"jest": "^26.1.0",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"@types/regenerator-runtime": "^0.13.0",
		"phin": "^3.5.0",
		"regenerator-runtime": "^0.13.7"
	}
}
